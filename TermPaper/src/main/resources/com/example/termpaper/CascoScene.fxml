      </CheckBox>
      <CheckBox fx:id="fallingObjects" layoutX="216.0" layoutY="130.0" mnemonicParsing="false" text="Falling objects">
         <font>
            <Font size="14.0" />
         </font>
      </CheckBox>
      <CheckBox fx:id="disaster" layoutX="216.0" layoutY="160.0" mnemonicParsing="false" text="Disaster">
         <font>
            <Font size="14.0" />
         </font>
      </CheckBox>
      <CheckBox fx:id="militaryRisks" layoutX="216.0" layoutY="190.0" mnemonicParsing="false" text="Military risks">
