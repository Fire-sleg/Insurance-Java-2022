         <cursor>
            <Cursor fx:constant="DEFAULT" />
         </cursor>
      </Button>
      <Label layoutX="208.0" layoutY="14.0" text="Select the risks">
         <font>
            <Font size="28.0" />
         </font>
      </Label>
